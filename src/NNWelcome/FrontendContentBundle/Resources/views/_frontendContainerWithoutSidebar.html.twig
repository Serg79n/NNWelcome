{% extends '::base.html.twig' %}

{% block body %}
    <div class="row-fluid">
        {% block breadCrumb %}
            {% render "FrontendContentBundle:Menu:breadCrumb" with {"alias" : content.alias } %}
        {% endblock %}
    </div>
    <div class="row-fluid">
        <div class="span2">
            <div id="sidebar_left">
                {% block sidebar %}{% endblock %}
            </div>
        </div>
        <div class="span10" id="main_content">

            <h1>{% block titlePage%}{% endblock %}</h1>

            {% block content %}{% endblock %}
        </div>
    </div>
{% endblock %}
