{% extends '::_adminContainer.html.twig' %}

{% block title %}{% trans with {'%title%' : entity.title } from "admin_object_type" %}Object Type edit - "%title%"{% endtrans %}{% endblock %}

{% block titlePage %}{% trans with {'%title%' : entity.title  } from "admin_object_type" %}Object Type edit - "%title%"{% endtrans %}{% endblock %}
    
{% block content %}
    <form action="{{ path('admin_object_type_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }} class="main_form">
        {{ form_widget(edit_form) }}
    </form>
    <form action="{{ path('admin_object_type_delete', {'id': entity.id }) }}" method="post"  class="delete_form" id="delete_form_{{entity.id}}">
        {{ form_widget(delete_form) }}
    </form>

    {% include "NticBundle:ObjectType:_modalDelete.html.twig" with {'id' : entity.id, 'name' : entity.title}%}
{% endblock %}

{% block sidebar %}
    {% include "NticBundle:ObjectType:_editFormActions.html.twig" %}
{% endblock %}
